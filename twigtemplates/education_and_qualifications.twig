{% extends "base.twig" %}
{% set Title_and_Description = fetch("Title_and_Description", {"id": _GET['0']})[0] %}
{% block title %} | {{Title_and_Description.Title}}{% endblock %}
{% block description %}{{Title_and_Description.description}}{% endblock %}
{% block og_url %}/educations_and_qualifications{% endblock %}
{% block og_title %} {{Title_and_Description.Title}}{% endblock %}
{% block og_description %}{{Title_and_Description.description}}{% endblock %}
{% block og_image %}{{_BASE}}/img/{{Title_and_Description.og_img}}{% endblock %}
{% block content %}

{% set item = fetch("Educations_and_Qualifications") %}

<!-- intro -->
<div class="flex padding-top-160 padding-bottom-160">
	<div class="line-black margin-left-64 margin-top-45 display-none-960"></div>
	<div class="flex flex-column">
		<div class="font-16 line-height-19 color-black margin-left-40 margin-bottom-6">Education and Qualifications</div>
		<div class="font-32 line-height-38 color-black width-550 margin-left-40 margin-bottom-50">{{item.Title}}</div>
		<div class="flex flex-900-column">
			<div class="flex flex-column margin-right-120 margin-900-bottom-40 margin-900-right-40">
				<div class="font-14 color-black font-300 line-height-22 margin-left-40 width-710">{{item.Description|raw}}</div>
			</div>
			<div class="flex flex-column">
				<div class="font-14 color-black font-300 line-height-22 width-710 margin-900-left-40 margin-900-right-40">{{item.description2|raw}}</div>
			</div>
		</div>
	</div>
</div>

<!-- testimonial -->
{% include 'testimonial.twig' %}

<!-- treatments -->
{% include 'treatments-slider.twig' %}

{% endblock %}